<h2>Book From Database</h2>
<table>
    <thead>
        <tr>
            <th>id</th>
            <th>isbn</th>
            <th>name</th>
            <th>price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (BookModel b in ViewBag.books)
        { 
            <tr>
                <td>@b.Id</td>
                <td>@b.Isbn</td>
                <td>@b.Name</td>
                <td>@b.Price</td>
            </tr>
        }
    </tbody>
</table>